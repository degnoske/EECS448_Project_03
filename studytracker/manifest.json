{
  "manifest_version": 2,

  "name": "Study Tracker",
  "description": "Tracks Study Time",
  "version": "1.0",

  "browser_action": {
    "default_icon": "book.png",
   "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
  "background",
    "activeTab",
    "contextMenus",
    "https://ajax.googleapis.com/"
  ]
  
}
